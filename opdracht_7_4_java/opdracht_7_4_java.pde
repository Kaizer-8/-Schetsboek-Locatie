void setup(){
  int dagen = 7;
  
  switch(dagen){
    case 1:
    println("het is maandag");
    case 2:
    println("het is dinsdag");
    case 3:
    println("het is woensdag");
    case 4:
    println("het is donderdag");
    case 5:
    println("het is vrijdag");
    case 6:
    println("het is zaterdag");
    case 7:
    println("het is zondag");
    break;
    default:
    println("er zijn maar 7 dagen in de week");
    break;
}
}
